<?xml version="1.0" encoding="utf-8"?>
<com.cauchy.androidexplore.view.TouchEventViewGroup xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.TouchEventActivity">

    <com.cauchy.androidexplore.view.TouchEventView
        android:id="@+id/view1"
        android:layout_width="0dp"
        android:layout_height="100dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/view2"
        app:layout_constraintTop_toTopOf="parent" />

    <com.cauchy.androidexplore.view.TouchEventView
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="100dp"
        app:layout_constraintLeft_toRightOf="@id/view1"
        app:layout_constraintRight_toLeftOf="@id/view3"
        app:layout_constraintTop_toTopOf="parent" />

    <com.cauchy.androidexplore.view.TouchEventView
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="100dp"
        app:layout_constraintLeft_toRightOf="@id/view2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</com.cauchy.androidexplore.view.TouchEventViewGroup>